{
    "name": "goldspecdigital/laravel-eloquent-uuid",
    "description": "An object oriented approach to generating OpenAPI specs, implemented in PHP.",
    "type": "library",
    "license": "MIT",
    "keywords": ["php", "laravel", "eloquent", "uuid"],
    "homepage": "https://github.com/goldspecdigital/laravel-eloquent-uuid",
    "authors": [
        {
            "name": "Matthew Inamdar",
            "email": "matt@goldspecdigital.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "GoldSpecDigital\\LaravelEloquentUUID\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "GoldSpecDigital\\LaravelEloquentUUID\\Tests\\": "test/"
      }
    },
    "require": {
        "php": ">=7.1.0",
        "illuminate/database": "~5.8.0",
        "illuminate/support": "~5.8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpunit/phpunit": "^8.2"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": [
            "@php vendor/bin/php-cs-fixer fix --config=.php_cs --allow-risky=yes --dry-run --diff --verbose --using-cache=no",
            "@php vendor/bin/phpunit"
        ],
        "test:style": "@php vendor/bin/php-cs-fixer fix --config=.php_cs --allow-risky=yes --dry-run --diff --verbose --using-cache=no",
        "test:unit": "@php vendor/bin/phpunit",
        "fix:style": "@php vendor/bin/php-cs-fixer fix --config=.php_cs --allow-risky=yes --diff --verbose --using-cache=no"
    }
}
